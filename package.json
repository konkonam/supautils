{
  "name": "supautils",
  "version": "1.0.2",
  "type": "module",
  "description": "Utilities to generate Zod schemas from Supabase/Postgres",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konkonam/supautils.git"
  },
  "homepage": "https://github.com/konkonam/supautils.git",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    "./cli": {
      "import": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    },
    "./codegen": {
      "import": "./dist/lib/codegen/index.js",
      "types": "./dist/lib/codegen/index.d.ts"
    },
    "./outputs": {
      "import": "./dist/outputs/index.js",
      "types": "./dist/outputs/index.d.ts"
    },
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.mts"
      ]
    }
  },
  "bin": {
    "supautils": "./bin/supautils"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "eco:bundlesize": "echo 'ðŸ“¦ Bundle generated ('$(du -sh dist | cut -f1)')'",
    "build:clean": "rm -rf dist",
    "build:types": "tsc",
    "build:js": "bun run build.ts",
    "prebuild": "bun build:clean",
    "build": "bun build:types && bun build:js",
    "postbuild": "bun eco:bundlesize",
    "dev": "bun playground/index.ts",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "supabase": "^2.22.6"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.9.0",
    "@stacksjs/dtsx": "^0.9.5",
    "@types/bun": "^1.2.20",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.6"
  },
  "keywords": [
    "supabase",
    "supabase-js",
    "validation",
    "schemas",
    "zod",
    "typescript",
    "codegen"
  ],
  "engines": {
    "node": "^18.x || ^20.x || ^22.x || ^24.x"
  },
  "trustedDependencies": [
    "esbuild"
  ],
  "dependencies": {
    "@supabase/postgres-meta": "^0.91.6",
    "citty": "^0.1.6",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "execa": "^9.6.0",
    "hookable": "^5.5.3",
    "pg": "^8.16.3",
    "toml": "^3.0.0",
    "zod": "^4.1.8"
  }
}